[ACER-1682]
  brandname = "Acer"
  modelname = "Aspire 1682 Wlmi"
  RAWCOMMAND[SETKEYCODES] = "e025 138 e026 139 e027 116 e029 214 e073 148 e074 149"
  [KEYS]
    Help		= 245 # Fn + F1	Help		KEY_HELP (138/e025)
    Setup		= 158 # Fn + F2	Acer eSetting	KEY_MENU (139/e026)
    PowerMan		= 222 # Fn + F3	Acer ePower Mgt	KEY_POWER (116/e027)
    AudioMute 		= 160 # Fn + F8	Mute
    AudioPlay|Pause 	= 162 # Fn + Home	Play
    AudioStop	 	= 164 # Fn + PgUp	Stop
    AudioPrev		= 144 # Fn + PgDn	Next Song
    AudioNext		= 153 # Fn + End	Previous Song
    AudioRaiseVolume	= 176 # Fn + ArrUp	Volume Up
    AudioLowerVolume	= 174 # Fn + ArrDn	Volume Down
    UserDefined1	= 159 # Top 'P'	Custom App.	KEY_PROG1 (148/e073)
    UserDefined2	= 151 # Top 'e'	Acer eMgr/Cust.	KEY_PROG2 (149/e074)
    WWW			= 178 # Top Web	Web Browser
    Mail		= 236 # Top Mail	Email Client
  [END KEYS]
[END ACER-1682]

# For ACER ASPIRE 1682 Wlmi
# Note : Num Pad already works, it doesnt need to be configured
# 
# Other hot keys :
#
# -KEY-----------------KEY CODE--------KEYB.---------FUNCTION--------ABOUT INPUT. H (CODES)----------REMARKS-----
# Sleep		=  ?		# Fn + F4	Sleep		:				:
# Video		= 214		# Fn + F5	Display (ext.?)	KEY_SWITCHVIDEOMODE (227/e029)	Never tested
# 			=  ?		# Fn + F6	Screen on/off	:				Works anyway
#			=  ?		# Fn + F7	Touchpad on/off	:				Works anyway
# Calculator	= 77		# Fn + F11	Num Lock	:				Works anyway
# Lock 		= 78		# Fn + F12	Scroll Lock	:				Never tested
# BrightnessLower	= 239		# Fn + ArrLe	Brightness Up	:				Works anyway
# BrightnessHigher	= 212		# Fn + ArrRi	Brightness Down	KEY_BRIGHTNESSUP (225/e06e)	Works anyway
# 			= (e033)	# Euro Key	Euro Symbol	:				Doesnt work
# 			= (e034)	# US $ Key	Dollar Symbol	:				Doesnt work

